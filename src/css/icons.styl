/*****************************
******** Custom icons**********
*****************************/

@font-face
  font-family: 'ordericons'
  src:  url('./fonts/order/order.eot?wifvp0')
  src:  url('./fonts/order/order.eot?wifvp0#iefix') format('embedded-opentype'),
    url('./fonts/order/order.ttf?wifvp0') format('truetype'),
    url('./fonts/order/order.woff?wifvp0') format('woff'),
    url('./fonts/order/order.svg?wifvp0#icomoon') format('svg')
  font-weight: normal
  font-style: normal

// GOM icon class
[class^="hi-"], [class*=" hi-"]
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'ordericons' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

.hi-order-won:before
  content: "\e900"

.hi-order-open:before
  content: "\e901"

.hi-order-inprocess:before
  content: "\e902"

.hi-order-closed:before
  content: "\e903";
